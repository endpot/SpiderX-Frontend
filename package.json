    "babel-plugin-dynamic-import-node": "2.3.3",
    "chalk": "2.4.2",
    "connect": "3.6.6",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^6.2.2",
    "html-webpack-plugin": "3.2.0",
    "lint-staged": "^10.3.0",
    "mockjs": "1.0.1-beta3",
    "prettier": "^2.1.1",
    "runjs": "^4.4.2",
    "sass": "^1.26.9",
    "sass-loader": "^8.0.2",
    "script-ext-html-webpack-plugin": "2.1.3",
    "script-loader": "0.7.2",
    "serve-static": "^1.14.1",
    "svg-sprite-loader": "4.1.3",
    "svgo": "1.2.2",
